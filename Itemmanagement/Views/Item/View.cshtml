@model Itemmanagement.Models.UpdateItem
@{

}
<h1>Update Item</h1>
<form method="post" action="View">
    <div class="mb-3">
        <label for="" class="form-label">Name</label>
        <input type="text" class="form-control" asp-for="Id" readonly>
    </div>
    <div class="mb-3">
        <label for="" class="form-label">Name</label>
        <input type="text" class="form-control" asp-for="Name">
    </div>
    <div class="mb-3">
        <label for="" class="form-label">Category</label>
        <input type="text" class="form-control" asp-for="Category">
    </div>
    <div class="mb-3">
        <label for="" class="form-label">Delivery</label>
        <input type="number" class="form-control" asp-for="Delivery">
    </div>
    <div class="mb-3">
        <label for="" class="form-label">price</label>
        <input type="date" class="form-control" asp-for="price">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
    <button type="submit" class="btn btn-danger"
    asp-action="Delete"
    asp-controller="StudentInformation">Delete</button>
</form>

