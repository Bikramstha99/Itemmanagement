@model List<Itemmanagement.Models.Domain.Item>
@{

}
<h1>Items</h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>Percentaage</th>
            <th>DateofBirth</th>
            <th>Department</th>
            <th> </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Category</td>
                <td>@item.Delivery</td>
                <td>@item.price</td>
                <td><a href="Item/View/@item.Id">View</a></td>
            </tr>
        }
    </tbody>
</table>